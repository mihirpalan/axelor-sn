<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.7.xsd">
	<module name="sn" package="com.axelor.sn.db"/>
	<entity name="ApplicationCredentials">
		<string name="apikey" title="API Key" required="true"></string>
		<string name="apisecret" title="API Secret" required="true"></string>
		<many-to-one name="userId" ref="com.axelor.auth.db.User" title="User"></many-to-one>
		<many-to-one name="snType" ref="SocialNetworking" title="Network Type" unique="true"></many-to-one>
		<string name="redirectUrl"  title="Redirect Url" help="You must Specify Same URL which you specify under Facebook Applicaton's  at WEBSITE WITH FACEBOOK LOGIN"></string>
	</entity>
</domain-models>