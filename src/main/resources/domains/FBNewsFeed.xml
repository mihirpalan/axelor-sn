<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.8.xsd">
	<module name="sn" package="com.axelor.sn.db" />
	<entity name="FBNewsFeed">
		<string name="feedid"/>
		<string name="name" title="Name of User"></string>
		<string name="contentdate" title="Date and Time"></string>
		<string name="type" title="Type of Content"></string>
		<string name="link" multiline="true"  title="Link (If Available)" max="1000"></string>
		<string name="message"  title="Message (If Available)" max="10000" multiline="true"></string>
		<boolean name="contentLike" title="Like..?" default="false"></boolean>
		<many-to-one name="curUser" ref="com.axelor.auth.db.User" />
	</entity>
</domain-models>
