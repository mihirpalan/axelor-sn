<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.7.xsd">
	<module name="sn" package="com.axelor.sn.db" />
	<entity name="PostMessage">
		<string name="content" multiline="true" namecolumn="true"></string>
		<string name="privacy"></string>
		<string name="acknowledgment"></string>
		<datetime name="postedTime" tz="true"></datetime>
		<many-to-one name="curUser" ref="com.axelor.auth.db.User"></many-to-one>
		<one-to-many name="comments" ref="com.axelor.sn.db.Comment" orphan="true"
			mappedBy="contentid" />
	</entity>
</domain-models>