<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.7.xsd">
	<module name="sn" package="com.axelor.sn.db" />
	<entity name="ConfigParameter">
		<many-to-one name="snType" ref="com.axelor.sn.db.SocialNetworking" required="true" title="Network Type"></many-to-one>
		<string name="paramFor" title="Functionality" required="true"></string>
		<integer name="paramLimit" title="Limit" min="0" />
<!-- 		<datetime name="paramSince" title="Since" tz="true"></datetime> -->
		<integer name="paramSince" title="Since" min="0"></integer>
<!-- 		<date name="paramUntil" title="Until"></date> -->
		<integer name="paramUntil" title="Until" min="0"></integer>
		<many-to-one name="curUser" ref="com.axelor.auth.db.User" unique="true"></many-to-one>
	</entity>
</domain-models>
