<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.7.xsd">

  <module name="sn" package="com.axelor.sn.db" />

  <entity name="GroupDiscussionComments">
	<many-to-one name="discussion" ref="com.axelor.sn.db.GroupDiscussion" orphan="true"/>
	<string name="commentId" title="Comment ID" />
	<string name="comment" title="Comment" multiline="true" max="4000"/>
	<datetime name="commentTime" title="Time" tz="true"/>
	<many-to-one name="curUser" ref="com.axelor.auth.db.User" />
	<string name="byUser" title="By"/> 
  </entity>

</domain-models>
