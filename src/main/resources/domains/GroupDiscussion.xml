<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.7.xsd">
	<module name="sn" package="com.axelor.sn.db"/>
	<entity name="GroupDiscussion">
		<string name="discussionId" title="Id"/>
		<string name="discussionSummary" title="Summary" max="4000" multiline="true"/>
		<string name="discussionTitle" title="Title" max="200"/>
		<string name="discussionBy" title="By"/>
		<datetime name="discussionTime" tz="true" title="Time"/>
		<many-to-one name="groupName" ref="com.axelor.sn.db.GroupMember"/>
		<many-to-one name="curUser" ref="com.axelor.auth.db.User" title="User"/>
		<one-to-many name="discussionComments" ref="com.axelor.sn.db.GroupDiscussionComments" mappedBy="discussion"/>
		<boolean name="discussionByCurrentUser" default="true"/>
	</entity>
</domain-models>