<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.7.xsd">
	<module name="sn" package="com.axelor.sn.db" />
	<entity name="FBPagePost">
	<many-to-one name="page" title="Page" ref="com.axelor.sn.db.FBPages" orphan="true"/>
	<string name="content" title="Content" max="1000" multiline="true"></string>
	<string name="acknowledgment" title="Acknowledgment"></string>
	<many-to-one name="curUser" ref="com.axelor.auth.db.User"/>
	<datetime name="postedTime" tz="true"></datetime>
	<one-to-many name="postedComments" ref="com.axelor.sn.db.FBPageComment" mappedBy="contentid"/>
	</entity>
</domain-models>
