<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_0.7.xsd">

	<grid name="import-contact-grid" title="Contact List"
		model="com.axelor.sn.db.ImportContact">
		<toolbar>
			<button name="butnCustom" title="Sync Contacts" onClick="com.axelor.sn.web.FBController:getAllContactsFB" />
			<button name="new" title="New" hidden="true" />
			<button name="save" title="Get Contacts" hidden="true" />
			<button name="edit" title="Edit" hidden="true" />
			<button name="delete" title="Delete" />
			<button name="refresh" title="Refresh" />
			<button name="search" title="Search" hidden="true" />
		</toolbar>
		<field name="snType"></field>
		<field name="name"></field>
		<field name="link"></field>
		<field name="userId" hidden="true"></field>
		<field name="curUser" hidden="true"></field>
	</grid>

	<form name="import-contact-formNew" title="Contact List"
		onNew="com.axelor.sn.web.FBController:getAllContactsFB">
		<field name="userId" readonly="true"></field>
		<field name="name" readonly="true"></field>
		<field name="link" readonly="true"></field>
		<field name="snType" readonly="true"></field>
		<field name="curUser" readonly="true" hidden="true"></field>
	</form>

	<action-record name="action-user-import-contact-user"
		model="com.axelor.sn.db.ImportContact">
		<field name="curUser" expr="eval:__user__" />
	</action-record>



</object-views>
